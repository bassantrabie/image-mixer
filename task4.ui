    background-color: rgb(255, 255, 255); /* Background color */
    color:black; /* Text color */
    font: 12px &quot;Arial&quot;; /* Adjust font size and style */
    padding: 6px;

}

/* Spinbox Arrows Buttons */
QSpinBox::up-button, QSpinBox::down-button {
    border: 1px solid black; /* Subtle border for arrows */
    border-radius: 25px; /* Rounded arrows */
    width: 8px; /* Adjust size */
    height: 8px;
    background-color:#A3A3A5; /* Light purple for arrows */
    margin: 4px; /* Prevent arrows from overlapping */
}

/* Up Arrow Style */
QSpinBox::up-arrow {
    width: 3px;
    height: 3px;
    border: solid black;
    border-width: 0 2px 2px 0;
    transform: rotate(-45deg); /* Correct arrow direction */
    background: none;
    margin: 2px;
}

/* Down Arrow Style */
QSpinBox::down-arrow {
    width: 3px;
    height: 3px;
    border: solid black;
    border-width: 0 2px 2px 0;
    transform: rotate(90deg); /* Correct arrow direction */
    background: none;
    margin: 2px;
}
